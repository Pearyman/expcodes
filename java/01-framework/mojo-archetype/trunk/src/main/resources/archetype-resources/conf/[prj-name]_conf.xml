#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<config hint="工程配置文件（常用配置）">

   <tabuPorts caption="端口禁忌表">
      <system caption="系统端口">
         <port>21</port>
         <port>22</port>
         <port id="http">80</port>
      </system>
      <coustom caption="自定义端口">
         <port id="mysql">3306</port>
         <port id="oracle">1521</port>
         <port id="socket">9998</port>
      </coustom>
   </tabuPorts>
   
   <datasources caption="数据源列表">
      <datasource id="TEST-DS" caption="测试数据源">
         <driver caption="数据源驱动类型" hint="数据库类型枚举,常用类型:
            oracle/sybase_ase/sybase_iq/mysql/mssql/sqlite">mysql</driver>
         <ip caption="IP地址">127.0.0.1</ip>
         <port caption="端口">3306</port>
         <name caption="库名">mysql_db</name>
         <username caption="账户">root</username>
         <password caption="密码">root</password>
         <charset default="cp936" caption="字符集" hint="数据库字符集">UTF-8</charset>
         <maximum-active-time default="300000" caption="连接有效时间(ms)" hint="若有长时操作需适当延长">300000</maximum-active-time>
      </datasource>
      
      <datasource id="TMP-DS" caption="临时数据源">
         <driver caption="数据源驱动类型" hint="数据库类型枚举,常用类型:
            oracle/sybase_ase/sybase_iq/mysql/mssql/sqlite">oracle</driver>
         <ip caption="IP地址">127.0.0.1</ip>
         <port caption="端口">1521</port>
         <name caption="库名">orcl_db</name>
         <username caption="账户">develop</username>
         <password caption="密码">develop</password>
         <charset default="cp936" caption="字符集" hint="数据库字符集">UTF-8</charset>
         <maximum-active-time default="300000" caption="连接有效时间(ms)" hint="若有长时操作需适当延长">300000</maximum-active-time>
      </datasource>
   </datasources>

   <sockets caption="Socket列表">
      <socket id="TEST-SOCK" caption="测试Socket">
         <ip default="127.0.0.1" caption="ip">192.168.1.2</ip>
         <port default="9998" caption="port">8889</port>
      </socket>
   </sockets>
  
</config>

